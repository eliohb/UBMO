                                                owl:onProperty geo:sfOverlaps ;
                                                owl:someValuesFrom ubmo:Vegetation
                                              ]
                                            )
                              ] ,
                              [ rdf:type owl:Class ;
                                owl:unionOf ( [ rdf:type owl:Restriction ;
                                                owl:onProperty geo:sfContains ;
                                                owl:someValuesFrom ubmo:VoieDEau
                                              ]
                                              [ rdf:type owl:Restriction ;
                                                owl:onProperty geo:sfOverlaps ;
                                                owl:someValuesFrom ubmo:VoieDEau
                                              ]
                                            )
                              ] ,
                              [ rdf:type owl:Class ;
                                owl:unionOf ( [ rdf:type owl:Restriction ;
                                                owl:onProperty geo:sfCrosses ;
                                                owl:someValuesFrom ubmo:ReseauAssainissement
                                              ]
                                              [ rdf:type owl:Restriction ;
                                                owl:onProperty geo:sfContains ;
                                                owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                owl:onClass ubmo:ReseauAssainissement
                                              ]
                                            )
                              ] ,
                              [ rdf:type owl:Class ;
                                owl:unionOf ( [ rdf:type owl:Restriction ;
                                                owl:onProperty geo:sfCrosses ;
                                                owl:someValuesFrom ubmo:ReseauUnitaire
                                              ]
                                              [ rdf:type owl:Restriction ;
                                                owl:onProperty geo:sfContains ;
                                                owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                owl:onClass ubmo:ReseauUnitaire
                                              ]
                                            )
                              ] ,
                              [ rdf:type owl:Class ;
                                owl:unionOf ( [ rdf:type owl:Restriction ;
                                                owl:onProperty geo:sfOverlaps ;
                                                owl:someValuesFrom ubmo:Batiment
                                              ]
                                              [ rdf:type owl:Restriction ;
                                                owl:onProperty geo:sfContains ;
                                                owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                owl:onClass ubmo:Batiment
                                              ]
                                            )
                              ] ,
                              [ rdf:type owl:Class ;
                                owl:unionOf ( [ rdf:type owl:Restriction ;
                                                owl:onProperty geo:sfOverlaps ;
                                                owl:someValuesFrom ubmo:CadastreBatiment
                                              ]
                                              [ rdf:type owl:Restriction ;
                                                owl:onProperty geo:sfContains ;
                                                owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                owl:onClass ubmo:CadastreBatiment
                                              ]
                                            )
                              ] ,
                              [ rdf:type owl:Class ;
                                owl:unionOf ( [ rdf:type owl:Restriction ;
                                                owl:onProperty geo:sfOverlaps ;
                                                owl:someValuesFrom ubmo:CadastreParcelle
                                              ]
                                              [ rdf:type owl:Restriction ;
                                                owl:onProperty geo:sfContains ;
                                                owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                owl:onClass ubmo:CadastreParcelle
                                              ]
                                            )
                              ] ,
                              [ rdf:type owl:Class ;
                                owl:unionOf ( [ rdf:type owl:Restriction ;
                                                owl:onProperty geo:sfContains ;
                                                owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                owl:onClass ubmo:ReseauEauPotable
                                              ]
                                              [ rdf:type owl:Restriction ;
                                                owl:onProperty geo:sfCrosses ;
                                                owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                owl:onClass ubmo:ReseauEauPotable
                                              ]
                                            )
                              ] ,
                              [ rdf:type owl:Restriction ;
                                owl:onProperty geo:hasGeometry ;
                                owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                owl:onClass geo:Geometry
                              ] ,
                              [ rdf:type owl:Restriction ;
                                owl:onProperty geo:sfContains ;
                                owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                owl:onClass ubmo:EquipementEauUsee
                              ] ,
                              [ rdf:type owl:Restriction ;
                                owl:onProperty geo:sfContains ;
                                owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                owl:onClass ubmo:IRIS
                              ] ,
                              [ rdf:type owl:Restriction ;
                                owl:onProperty geo:sfContains ;
                                owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                owl:onClass ubmo:WaterWasteManhole
                              ] ,
                              [ rdf:type owl:Restriction ;
                                owl:onProperty geo:sfWithin ;
                                owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                owl:onClass ubmo:Commune
                              ] ;
              ignf:geodeticDatum ignf:GeodeticCRS ;
              rdfs:comment """la notion de grand quartier est définie comme un groupement de plusieurs îlots contigus à l'intérieur d'une même commune. Lorsque cette commune est partagée en plusieurs cantons, les limites de quartiers peuvent ne pas respecter les limites de cantons. La taille des quartiers est très variable. Elle doit respecter certaines normes de population. Ainsi, une commune de 20 000 habitants n'est généralement pas découpée en plus de deux ou trois quartiers; de même, seules quelques communes de moins de 10 000 habitants sont découpées en quartiers (source INSEE).
Sur le territoire de Grand Poitiers, actuellement seule la commune de Poitiers est concernée par la notion de quartier IRIS."""@fr ;
              rdfs:definition "Administrative division of a city. It is part of a city with certain characteristics or a certain unity."@en ;
              skos:definition "Division administrative d'une ville."@fr ,
                              "Environs immédiats, dans une ville, du lieu où on se trouve et, en particulier, du lieu d'habitation"@fr ,
                              "Partie d'une ville ayant certaines caractéristiques ou une certaine unité"@fr ;
              skos:prefLabel "District"@en .


###  http://w3id.org/UrbanAndBuildingMultiscaleOntology#Regard
ubmo:Regard rdf:type owl:Class ;
            rdfs:subClassOf ubmo:CombinedSewerSystem ,
                            ubmo:DrinkingWaterSystem ,
                            [ rdf:type owl:Restriction ;
                              owl:onProperty geo:hasGeometry ;
                              owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                              owl:onClass <http://www.opengis.net/ont/sf#Point>
                            ] ,
                            [ rdf:type owl:Restriction ;
                              owl:onProperty geo:sfWithin ;
                              owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                              owl:onClass ubmo:Quartier
                            ] ;
            rdfs:definition "Ce sont des structures d'accès qui permettent l'inspection, l'entretien, le nettoyage et éventuellement la redirection des flux dans le système unitaire"@fr ,
                            "These are access structures that enable inspection, maintenance, cleaning and, if necessary, redirection of flows in the unitary system."@en ;
            rdfs:label "UnitaryWaterManhole"@en .


###  http://w3id.org/UrbanAndBuildingMultiscaleOntology#Region
ubmo:Region rdf:type owl:Class ;
            rdfs:subClassOf owl:Thing ,
                            [ rdf:type owl:Restriction ;
                              owl:onProperty geo:hasGeometry ;
                              owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                              owl:onClass geo:Geometry
                            ] ,
                            [ rdf:type owl:Restriction ;
                              owl:onProperty geo:sfContains ;
                              owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                              owl:onClass ubmo:Departement
                            ] ,
                            [ rdf:type owl:Restriction ;
                              owl:onProperty geo:sfWithin ;
                              owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                              owl:onClass ubmo:Pays
                            ] ;
            ignf:geodeticDatum ignf:GeodeticCRS ;
            rdfs:definition "Contient l'ensemble des régions françaises, y compris les DOM et Mayotte."@fr ,
                            "Territory whose variable extent is determined either by an administrative or economic unit, or by the similarity of relief, climate and vegetation, or by a cultural community"@en ;
            skos:definition "Territoire dont l'étendue variable est déterminée soit par une unité administrative ou économique, soit par la similitude du relief, du climat et de la végétation, soit par une communauté culturelle"@fr ;
            skos:prefLabel "Region"@en .


###  http://w3id.org/UrbanAndBuildingMultiscaleOntology#ReseauAssainissement
ubmo:ReseauAssainissement rdf:type owl:Class ;
                          rdfs:subClassOf ubmo:SystemEauUsee ,
                                          [ rdf:type owl:Class ;
                                            owl:unionOf ( [ rdf:type owl:Restriction ;
                                                            owl:onProperty geo:sfTouches ;
                                                            owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                            owl:onClass ubmo:ReseauAssainissement
                                                          ]
                                                          [ rdf:type owl:Restriction ;
                                                            owl:onProperty geo:sfTouches ;
                                                            owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                            owl:onClass ubmo:WaterWasteManhole
                                                          ]
                                                        )
                                          ] ,
                                          [ rdf:type owl:Class ;
                                            owl:unionOf ( [ rdf:type owl:Restriction ;
                                                            owl:onProperty geo:sfCrosses ;
                                                            owl:maxQualifiedCardinality "2"^^xsd:nonNegativeInteger ;
                                                            owl:onClass ubmo:Quartier
                                                          ]
                                                          [ rdf:type owl:Restriction ;
                                                            owl:onProperty geo:sfWithin ;
                                                            owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                            owl:onClass ubmo:Quartier
                                                          ]
                                                        )
                                          ] ,
                                          [ rdf:type owl:Restriction ;
                                            owl:onProperty geo:hasGeometry ;
                                            owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                            owl:onClass <http://www.opengis.net/ont/sf#LineString>
                                          ] ;
                          ignf:geodeticDatum ignf:GeodeticCRS ;
                          rdfs:comment "Contient les informations que le service a pu trouver sur les plans papiers, par la connaissance terrain des agents ainsi que l'information extraite des archives. L'échelle de référence est du 2500ème"@fr ;
                          rdfs:definition "Elles sont collectées par les réseaux d'assainissement (égoûts), qui les acheminent vers une station d'épuration collective, sauf quand l’habitation n’est pas reliée au réseau public"@fr ,
                                          "Set of works built by man to channel rainwater and wastewater within a built-up area. Most of these works are underground pipelines interconnected"@en ;
                          skos:prefLabel "SewerNetwork"@en .


###  http://w3id.org/UrbanAndBuildingMultiscaleOntology#ReseauEauPotable
ubmo:ReseauEauPotable rdf:type owl:Class ;
                      rdfs:subClassOf ubmo:DrinkingWaterSystem ,
                                      [ rdf:type owl:Class ;
                                        owl:unionOf ( [ rdf:type owl:Restriction ;
                                                        owl:onProperty geo:sfCrosses ;
                                                        owl:maxQualifiedCardinality "2"^^xsd:nonNegativeInteger ;
                                                        owl:onClass ubmo:Quartier
                                                      ]
                                                      [ rdf:type owl:Restriction ;
                                                        owl:onProperty geo:sfWithin ;
                                                        owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                        owl:onClass ubmo:Quartier
                                                      ]
                                                    )
                                      ] ,
                                      [ rdf:type owl:Restriction ;
                                        owl:onProperty geo:hasGeometry ;
                                        owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                        owl:onClass <http://www.opengis.net/ont/sf#LineString>
                                      ] ;
                      rdfs:definition "A drinking water network is a set of pipes and equipment organized to allow the circulation and distribution of drinking water to the population of a community or several communities"@en ,
                                      "Un réseau d’eau potable est un ensemble de conduites et d’équipements organisés pour permettre la circulation et la distribution de l’eau potable vers la population d’une collectivité ou de plusieurs collecticvités"@fr ;
                      skos:prefLabel "DrinkingWaterNetwork"@en .


###  http://w3id.org/UrbanAndBuildingMultiscaleOntology#ReseauUnitaire
ubmo:ReseauUnitaire rdf:type owl:Class ;
                    rdfs:subClassOf ubmo:CombinedSewerSystem ,
                                    [ rdf:type owl:Class ;
                                      owl:unionOf ( [ rdf:type owl:Restriction ;
                                                      owl:onProperty geo:sfCrosses ;
                                                      owl:maxQualifiedCardinality "2"^^xsd:nonNegativeInteger ;
                                                      owl:onClass ubmo:Quartier
                                                    ]
                                                    [ rdf:type owl:Restriction ;
                                                      owl:onProperty geo:sfWithin ;
                                                      owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                      owl:onClass ubmo:Quartier
                                                    ]
                                                  )
                                    ] ,
                                    [ rdf:type owl:Restriction ;
                                      owl:onProperty geo:hasGeometry ;
                                      owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                      owl:onClass <http://www.opengis.net/ont/sf#LineString>
                                    ] ;
                    rdfs:definition "all pipes and conduits that collect both wastewater and rainwater"@en ,
                                    "l'ensemble des canalisations et conduites qui collectent à la fois les eaux usées et les eaux pluviales"@fr ;
                    rdfs:label "CombinedNetwork"@fr .


###  http://w3id.org/UrbanAndBuildingMultiscaleOntology#SystemEauUsee
ubmo:SystemEauUsee rdf:type owl:Class ;
