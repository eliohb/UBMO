                                      "Ouvrage construit au-dessous du niveau du sol pour servir au passage d'une voie de communication, à une galerie d'amenée ou d'évacuation des eaux, au stockage de produits, etc"@fr ;
                      skos:prefLabel "UndergroundSystem"@en .


###  http://w3id.org/UrbanAndBuildingMultiscaleOntology#Vegetation
ubmo:Vegetation rdf:type owl:Class ;
                rdfs:subClassOf ubmo:Nature ,
                                [ rdf:type owl:Class ;
                                  owl:unionOf ( [ rdf:type owl:Restriction ;
                                                  owl:onProperty geo:sfOverlaps ;
                                                  owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                  owl:onClass ubmo:Quartier
                                                ]
                                                [ rdf:type owl:Restriction ;
                                                  owl:onProperty geo:sfWithin ;
                                                  owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                  owl:onClass ubmo:Quartier
                                                ]
                                              )
                                ] ;
                owl:disjointWith ubmo:VoieDEau ;
                ignf:geodeticDatum ignf:GeodeticCRS ;
