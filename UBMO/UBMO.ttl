                                   [ rdf:type owl:Class ;
                                     owl:unionOf ( [ rdf:type owl:Restriction ;
                                                     owl:onProperty geo:sfOverlaps ;
                                                     owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                     owl:onClass ubmo:Commune
                                                   ]
                                                   [ rdf:type owl:Restriction ;
                                                     owl:onProperty geo:sfWithin ;
                                                     owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                     owl:onClass ubmo:Commune
                                                   ]
                                                 )
                                   ] ,
                                   [ rdf:type owl:Restriction ;
                                     owl:onProperty geo:hasGeometry ;
                                     owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                     owl:onClass <http://www.opengis.net/ont/sf#GeometryCollection>
                                   ] ;
                   ignf:geodeticDatum ignf:GeodeticCRS ;
                   skos:definition " a wastewater collection system where all water (wastewater and stormwater) flows through a single pipe and mixes "@fr ,
                                   "Aussi connu sous le nom de réseau d'assainissement, c'est un système conçu pour collecter et transporter les eaux usées domestiques et industrielles vers une station d'épuration pour traitement avant leur rejet dans l'environnement"@fr ;
                   skos:prefLabel "SewerSystem"@en .


###  http://w3id.org/UrbanAndBuildingMultiscaleOntology#SystemSouterrain
ubmo:SystemSouterrain rdf:type owl:Class ;
                      rdfs:subClassOf [ rdf:type owl:Class ;
                                        owl:unionOf ( [ rdf:type owl:Restriction ;
                                                        owl:onProperty geo:sfOverlaps ;
                                                        owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                        owl:onClass ubmo:Departement
                                                      ]
                                                      [ rdf:type owl:Restriction ;
                                                        owl:onProperty geo:sfWithin ;
